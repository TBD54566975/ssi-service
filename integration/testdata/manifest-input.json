{
  "issuerDID":"{{.IssuerID}}",
  "issuerName":"Issuer Name Lol",
  "format":{
    "jwt":{
      "alg":[
        "EdDSA"
      ]
    }
  },
  "outputDescriptors":[
    {
      "id":"kyc_credential",
      "schema":"{{.SchemaID}}"
    }
  ],
  "presentationDefinition":{
    "id":"32f54163-7166-48f1-93d8-ff217bdb0653",
    "name":"KYC Requirements",
    "purpose":"TBD",
    "format":{
      "jwt":{
        "alg":[
          "EdDSA"
        ]
      }
    },
    "input_descriptors":[
      {
        "id":"kyc1",
        "name":"Personal Info",
        "constraints":{
          "subject_is_issuer":"required",
          "fields":[
            {
              "id":"givenName",
              "path":[
                "$.credentialSubject.givenName"
              ],
              "filter":{
                "type":"string",
                "pattern":"[a-zA-Z \\-\\.].+"
              }
            },
            {
              "id":"additionalName",
              "path":[
                "$.credentialSubject.additionalName"
              ],
              "filter":{
                "type":"string",
                "pattern":"[a-zA-Z \\-\\.].+"
              }
            },
            {
              "id":"familyName",
              "path":[
                "$.credentialSubject.familyName"
              ],
              "filter":{
                "type":"string",
                "pattern":"[a-zA-Z \\-\\.].+"
              }
            },
            {
              "id":"birthDate",
              "path":[
                "$.credentialSubject.birthDate"
              ],
              "filter":{
                "type":"string",
                "format":"date"
              }
            },
            {
              "id":"taxID",
              "path":[
                "$.credentialSubject.taxID"
              ],
              "filter":{
                "type":"string"
              }
            },
            {
              "id":"postalAddress",
              "path":[
                "$.credentialSubject.postalAddress"
              ],
              "filter":{
                "type":"object",
                "properties":{
                  "addressCountry":{
                    "type":"string"
                  },
                  "addressLocality":{
                    "type":"string"
                  },
                  "addressRegion":{
                    "type":"string"
                  },
                  "postalCode":{
                    "type":"string"
                  },
                  "streetAddress":{
                    "type":"string"
                  }
                },
                "required":[
                  "addressCountry",
                  "addressLocality",
                  "addressRegion",
                  "postalCode",
                  "streetAddress"
                ]
              }
            }
          ]
        }
      }
    ]
  }
}